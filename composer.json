{
    "name": "kiqstyle/eloquent-versionable",
    "description": "Versioning behaviour for eloquent models",
    "authors": [
        {
            "name": "Caique Pedro dos Santos",
            "email": "caique_pedro@hotmail.com"
        }
    ],
    "keywords":
        [
            "version",
            "versioning",
            "eloquent",
            "model",
            "laravel",
            "behaviour"
        ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "illuminate/database": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "squizlabs/php_codesniffer": "*",
        "nunomaduro/phpinsights": "^2.11",
        "phpmd/phpmd": "^2.15",
        "pestphp/pest": "^1.23"
    },
    "autoload": {
        "psr-4": {
            "Kiqstyle\\EloquentVersionable\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kiqstyle\\EloquentVersionable\\VersioningServiceProvider"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kiqstyle\\EloquentVersionable\\Test\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "pestphp/pest-plugin": true
        }
    }
}
